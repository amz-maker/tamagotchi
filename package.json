{
  "name": "phaser-electron-typescript-parcel",
  "version": "1.0.0",
  "description": "phaser electron typescript parcel template",
  "main": "build/main.js",
  "scripts": {
    "----- Starter -----": "",
    "build": "rimraf build && parcel build src/main.ts src/index.html --public-url ./ -d build --out-file=main --target=electron",
    "parcel-start": "parcel src/index.html",
    "electron-start": "electron build/main.js",
    "electron-debug-chrome": "cross-env ELECTRON_DEBUG=true electron build/main.js",
    "electron-debug-vscode": "cross-env ELECTRON_DEBUG=vscode electron --remote-debugging-port=9223 build/main.js",
    "----- Cleaner -----": "",
    "superclean": "rimraf .cache node_modules build dist",
    "clean": "rimraf build dist",
    "dist": "yarn build && electron-builder"
  },
  "homepage": "https://github.com/distantcam/phaser-electron-typescript-parcel",
  "bugs": {
    "url": "https://github.com/distantcam/phaser-electron-typescript-parcel/issues"
  },
  "build": {
    "appId": "parcel.electron",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "build/*"
    ]
  },
  "keywords": [
    "phaser",
    "electron"
  ],
  "author": "distantcam",
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.54.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "download-cli": "^1.1.1",
    "electron": "^9.4.0",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.5",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}